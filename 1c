#include <stdio.h>
int main() {
    int a;
    printf("enter temperature in centigrade:");
    scanf("%d",&a);
    if(a<0)
    {printf("Its freezing weather");}
    else if(a>=0&&a<=10)
    {printf("Very cold weather");}
    else if(a>=10&&a<=20)
    {printf("cold weather");}
    else if(a>=20&&a<=30)
    {printf("Normal in Temp");}
    else if(a>=30&&a<=40)
    {printf("Its Hot");}
    else
    {printf("Its Hot");}
    return 0;
}
