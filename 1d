#include <stdio.h>

int main() {
    int a,b;
    printf("enter the order of matrix:");
    scanf("%d %d",&a,&b);
    int r[a][b];
    for(int i=0;i<a;i++)
    {for(int j=0;j<b;j++)
    {   printf("enter element a%d%d:",i,j);
        scanf("%d",&r[i][j]);
    }}
   
    printf("given matrix\n");
      for(int i=0;i<a;i++)
    {for(int j=0;j<b;j++)
    {printf("%d ",r[i][j]);
    }
        printf("\n");
    }
   
    printf("\n");
     printf("Transpose of matrix\n");
       for(int i=0;i<a;i++)
    {for(int j=0;j<b;j++)
    {printf("%d ",r[j][i]);
    }
        printf("\n");
    }
   
     
    return 0;
}
